<Application x:Class="TestMvvmToolKit.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TestMvvmToolKit"
             xmlns:vm="clr-namespace:TestMvvmToolKit.ViewModels"
             xmlns:views="clr-namespace:TestMvvmToolKit.Views"             
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:FormViewModels}">
            <views:FormUserControl1/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:ManagerViewModels}">
            <views:ManagerUserControl1/>
        </DataTemplate>
    </Application.Resources>
</Application>
